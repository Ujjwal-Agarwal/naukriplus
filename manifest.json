{
  "manifest_version": 2,
  "name": "NaukriPlusPlus",
  "version": "1.0",

  "description": "Adds accurate numbers to applicants for each job.",

  "permissions" : [
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },

  "content_scripts": [
    {
      "matches": ["*://www.naukri.com/*"],
      "js": ["naukriplus.js"],
      "run_at" : "document_end"
    }
  ]
}